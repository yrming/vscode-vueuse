    "fp-ts": "^2.12.1",
    "fs-extra": "^10.1.0",
    "happy-dom": "^7.6.6",
    "inferred-types": "^0.22.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup": "^2.77.0",
    "typescript": "~4.7.4",
    "unocss": "^0.45.29",
    "vite": "^3.0.9",
