    "@rollup/pluginutils": "^4.2.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/fs-extra": "^9.0.13",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.56",
    "@types/vscode-webview": "^1.57.0",
    "@unocss/preset-icons": "^0.45.30",
